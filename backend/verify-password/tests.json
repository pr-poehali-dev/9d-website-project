{
  "tests": [
    {
      "name": "Test valid password",
      "method": "POST",
      "path": "/",
      "body": {
        "password": "6745Q-"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "valid": true
      }
    },
    {
      "name": "Test invalid password",
      "method": "POST",
      "path": "/",
      "body": {
        "password": "wrong"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "valid": false
      }
    }
  ]
}
